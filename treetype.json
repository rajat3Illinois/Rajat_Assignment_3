{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 500,
    "height": 300,
    "data": {"url": "City_Owned_Trees.csv"},
    "title": "Tree Type Bar Plot based on Cond",
    "mark":"bar",
    "encoding": {
      "x": {"aggregate":"count", "field": "GROW", "type": "quantitative", "title": "Count"},
      "y": {"field": "TREETYPE"},
      "color": {"field": "COND", "type": "nominal"}   
  }
}